
[
  {
    "line1": "An owner of the old thatched hut", 
    "line2": "Is also changed to another", 
    "line3": "It’s the Doll’s Festival."
  },
  {
    "line1": "Spring is passing.", 
    "line2": "The birds cry, and the fishes’ eyes are",  
    "line3": " With tears."
  },
  {
    "line1": "The summer grasses.",
    "line2": "All that remains",
    "line3": "Of warriors’ dreams."
  },
  {
    "line1": "The early summer rain",
    "line2": "Falls and falls yet",
    "line3": "Leaves Hikari-do Hall untouched."
  }, 
  {
    "line1": "Oh, tranquility!",
    "line2": "Penetrating the very rock,",
    "line3": "A cicada's voice."
  }, 
  {
    "line1": "The rains of summer join together.",
    "line2": "How swift it is",
    "line3": "Mogami River."
  }, 
  {
    "line1": "The old pond.",
    "line2": "A frog leaps in.",
    "line3": "Sound of the water."
  }, 
  {
    "line1": "When you say something,",
    "line2": "The lips feel cold.",
    "line3": "The Autumn wind."
  }, 
  {
    "line1": "Sick on my journey,",
    "line2": "Only my dreams will wander",
    "line3": "These desolate moors."
  }, 
  {
    "line1": "Snowflakes like cookies",
    "line2": "Changing into white strings",
    "line3": "The willow"
  }, 
  {
    "line1": "That handsome man",
    "line2": "Clearly is no longer living.",
    "line3": "A rain-hidden moon"
  }, 
  {
    "line1": "A pair of deer",
    "line2": "Hair on hair in agreement,",
    "line3": "With hair so hard."
  }, 
   {
    "line1": "Without waiting",
    "line2": "For the cuckoo has he come",
    "line3": "Selling vegetables"
  }, 
  {
    "line1": "Grass of forgetting",
    "line2": "Picked for a rice soup",
    "line3": "The end of the year"
  },
   {
    "line1": "Autumn has come",
    "line2": "Loving a wife with stars",
    "line3": "On buckskin"
  },  
  {
    "line1": "It had to be.",
    "line2": "It had to be until",
    "line3": "The end of the year"
  } 
]
